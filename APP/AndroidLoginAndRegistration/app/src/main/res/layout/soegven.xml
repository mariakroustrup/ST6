<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/btnFoelgVen"
        android:layout_width="150dp"
        android:layout_gravity="right"
        android:layout_height="wrap_content"
        android:layout_weight="0.00"
        android:text="Følg ven"
        android:visibility="visible"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:id="@+id/btnFjernVen"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_gravity="left"
        android:layout_weight="0.00"
        android:text="Fjern ven"
        android:visibility="visible"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/Beloenniger1"
        android:layout_width="190dp"
        android:layout_height="50dp"
        android:layout_centerHorizontal="true"
        android:layout_weight="0.00"
        android:text="Belønninger"
        android:textAlignment="center"
        android:textSize="35dp"
        android:layout_below="@+id/btnFoelgVen" />

    <TextView
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:text=" Navn:"
        android:textSize="20dp"
        android:id="@+id/textView7"
        android:layout_marginTop="22dp"
        android:layout_below="@+id/Beloenniger1"
        android:layout_alignLeft="@+id/textView5"
        android:layout_alignStart="@+id/textView5" />

    <TextView
        android:id="@+id/venNavn"
        android:layout_width="260dp"
        android:layout_height="30dp"
        android:textSize="20dp"
        android:layout_alignBaseline="@+id/textView7"
        android:layout_alignBottom="@+id/textView7"
        android:layout_toRightOf="@+id/textView7"
        android:layout_toEndOf="@+id/textView7" />

    <TextView
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:text=" MedlemsID:"
        android:textSize="20dp"
        android:id="@+id/textView5"
        android:layout_marginTop="51dp"
        android:layout_below="@+id/Beloenniger1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="12dp"
        android:layout_marginStart="12dp" />

    <TextView
        android:id="@+id/venMedlemsid"
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:textSize="20dp"
        android:layout_alignBaseline="@+id/textView5"
        android:layout_alignBottom="@+id/textView5"
        android:layout_toRightOf="@+id/textView7"
        android:layout_toEndOf="@+id/textView7" />

    <TextView
        android:id="@+id/gange"
        android:layout_width="180dp"
        android:layout_height="30dp"
        android:text="Antal træninger:"
        android:layout_gravity="center_horizontal"
        android:textSize="25dp"
        android:layout_marginTop="19dp"
        android:layout_below="@+id/textView5"
        android:layout_alignLeft="@+id/Beloenniger1"
        android:layout_alignStart="@+id/Beloenniger1" />

    <ImageView
        android:id="@+id/starantal"
        android:layout_width="340dp"
        android:layout_height="50dp"
        android:layout_gravity="center"
        android:src="@drawable/star0"
        android:layout_below="@+id/gange"
        android:layout_centerHorizontal="true" />

    <TextView
        android:id="@+id/beloentid"
        android:layout_width="50dp"
        android:layout_height="30dp"
        android:text="Tid:"
        android:layout_gravity="center_horizontal"
        android:textSize="25dp"
        android:layout_below="@+id/starantal"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/startid"
        android:layout_width="340dp"
        android:layout_height="50dp"
        android:layout_gravity="center"
        android:src="@drawable/star0"
        android:layout_below="@+id/beloentid"
        android:layout_alignLeft="@+id/starantal"
        android:layout_alignStart="@+id/starantal" />

    <TextView
        android:id="@+id/beloenafstand"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:text="Afstand:"
        android:layout_gravity="center"
        android:textSize="25dp"
        android:layout_below="@+id/startid"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/starafstand"
        android:layout_width="340dp"
        android:layout_height="50dp"
        android:layout_gravity="center"
        android:src="@drawable/star0"
        android:layout_below="@+id/beloenafstand"
        android:layout_alignLeft="@+id/startid"
        android:layout_alignStart="@+id/startid" />

    <TextView
        android:id="@+id/kondition"
        android:layout_width="210dp"
        android:layout_height="30dp"
        android:text="Konditionstræning:"
        android:layout_gravity="center"
        android:textSize="25dp"
        android:layout_below="@+id/starafstand"
        android:layout_alignLeft="@+id/gange"
        android:layout_alignStart="@+id/gange" />

    <ImageView
        android:id="@+id/starkondi"
        android:layout_width="340dp"
        android:layout_height="50dp"
        android:layout_gravity="center"
        android:src="@drawable/star0"
        android:layout_below="@+id/kondition"
        android:layout_alignLeft="@+id/starafstand"
        android:layout_alignStart="@+id/starafstand" />

</RelativeLayout>