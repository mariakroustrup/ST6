<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/Beloenniger1"
        android:layout_width="220dp"
        android:layout_height="50dp"
        android:layout_gravity="center"
        android:layout_marginBottom="20dp"
        android:layout_marginTop="40dp"
        android:layout_weight="0.00"
        android:text="Belønninger"
        android:textAlignment="center"
        android:textSize="40dp" />

    <TextView
        android:id="@+id/gange"
        android:layout_width="180dp"
        android:layout_height="30dp"
        android:text="Antal træninger:"
        android:layout_gravity="center_horizontal"
        android:textSize="25dp" />


    <RatingBar
        android:layout_margin="5dp"
        android:id="@+id/ratingBar"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:layout_gravity="center"
        android:clickable="true"
        android:numStars="6" />


    <TextView
        android:id="@+id/beloentid"
        android:layout_width="50dp"
        android:layout_height="30dp"
        android:text="Tid:"
        android:layout_gravity="center_horizontal"
        android:textSize="25dp" />

    <RatingBar
        android:layout_margin="5dp"
        android:id="@+id/ratingBar"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:layout_gravity="center"
        android:clickable="true"
        android:numStars="6" />

    <TextView
        android:id="@+id/beloenafstand"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:text="Afstand:"
        android:layout_gravity="center"
        android:textSize="25dp" />

    <RatingBar
        android:layout_margin="5dp"
        android:id="@+id/ratingBar"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:layout_gravity="center"
        android:clickable="true"
        android:numStars="6" />


    <TextView
        android:id="@+id/kondition"
        android:layout_width="210dp"
        android:layout_height="30dp"
        android:text="Konditionstræning:"
        android:layout_gravity="center"
        android:textSize="25dp" />

    <RatingBar
        android:layout_margin="5dp"
        android:id="@+id/ratingBar"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:layout_gravity="center"
        android:clickable="true"
        android:numStars="6" />

    <TextView
        android:id="@+id/venMedlemsid"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:layout_marginTop="10dp"
        android:text="MedlemsID:"
        android:layout_marginLeft="20dp"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/venFornavn"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:text="Fornavn:"
        android:layout_marginLeft="20dp"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/venEfternavn"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:text="Efternavn:"
        android:layout_marginLeft="20dp"
        android:textSize="20dp" />

</LinearLayout>